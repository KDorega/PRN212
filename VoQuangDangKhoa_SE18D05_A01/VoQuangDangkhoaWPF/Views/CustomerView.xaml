<Window x:Class="VoQuangDangKhoaWPF.Views.CustomerView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Customer Dashboard" Height="400" Width="600">
    <Grid Margin="20">
        <StackPanel>
            <TextBlock Text="Customer Dashboard" FontSize="20" FontWeight="Bold" Margin="0,0,0,10"/>

            
            <TextBlock x:Name="CustomerInfo" Text="Welcome, Customer!" Margin="0,0,0,10"/>

            
            <Button Content="View My Bookings" Click="ViewBookings_Click" Margin="0,0,0,10"/>

            
            <Button Content="Logout" Click="Logout_Click" Margin="0,0,0,10"/>
        </StackPanel>
    </Grid>
</Window>